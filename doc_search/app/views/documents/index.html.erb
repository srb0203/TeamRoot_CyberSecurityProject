<%= stylesheet_link_tag 'documents.scss' %>

<div class="container" id="main-page">
  <div class="Header">
    <%= link_to 'Add New Document', new_document_path %>
    <p id="notice"><%= notice %></p>
  </div>
  <div class="wrapper">
    <h1 id= "myLogo">ROOTS</h1>
      <div id = "search">
        <form action="/search" accept-charset="UTF-8" method="get">
          <input placeholder="Search..." type="text" name="q" id="q"></input>
          <input id="search-submit" type="submit" value="Search"></input>
        </form>
      </div>
    </div>
</div>
